config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "MicroGame",
"language": "GDScript",
"path": "res://Scripts/MicroGame.gd"
} ]
_global_script_class_icons={
"MicroGame": ""
}

[application]

config/name="HaachamaWare"
run/main_scene="res://Scenes/Loading.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="HaachamaWare"
boot_splash/image="res://Assets/Sprites/splash.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[autoload]

Global="*res://Scripts/Global.gd"

[debug]

settings/crash_handler/message="Please report this to the devs. Thanks!"

[display]

window/size/width=960
window/size/height=640
window/size/resizable=false
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/AsepriteWizard/plugin.cfg" )

[global]

"clear color"=false

